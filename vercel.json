{
  "framework": "vite",
  "git": {
    "deploymentEnabled": {
      "main": true
    }
  },
  "ignoreCommand": "git diff --quiet HEAD^ HEAD -- .",
  "rewrites": [
    {
      "source": "/api/kalshi/:path*",
      "destination": "/api/kalshi/:path*"
    },
    {
      "source": "/((?!api/).*)",
      "destination": "/index.html"
    }
  ]
}
